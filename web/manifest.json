{
    "manifest_version": 2,
    "name": "skyeng",
    "version": "1.0",
    "browser_action": {
        "default_title": "Skyeng Grammar Filter",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*",
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "jquery.min.js",
            "parcer.js"
        ]
    }]
}